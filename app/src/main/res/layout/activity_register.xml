<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ksulogo"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="-73dp" />

    <RelativeLayout
        android:layout_width="266dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_height="match_parent"
        tools:layout_width="match_parent">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView19"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/textView19"
            android:layout_marginEnd="14dp"
            android:layout_marginRight="14dp"
            android:layout_marginTop="110dp"
            android:text="الاسم الأول"
            android:textSize="18sp"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="الاسم الأخير"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            android:layout_below="@+id/editText9"
            android:layout_alignLeft="@+id/textView13"
            android:layout_alignStart="@+id/textView13" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView16"
            android:layout_alignRight="@+id/textView16"
            android:text="كلمة المرور"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            android:layout_below="@+id/editText10" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView17"
            android:layout_marginEnd="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="55dp"
            android:text=" البريد الالكتروني"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            tools:layout_width="150dp" />

        <EditText
            android:id="@+id/editText9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:layout_below="@+id/textView13"
            android:layout_alignRight="@+id/textView19"
            android:layout_alignEnd="@+id/textView19" />

        <EditText
            android:id="@+id/editText10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:layout_below="@+id/textView16"
            android:layout_alignLeft="@+id/editText9"
            android:layout_alignStart="@+id/editText9" />

        <EditText
            android:id="@+id/editText14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword"
            android:layout_below="@+id/textView17"
            android:layout_alignRight="@+id/textView19"
            android:layout_alignEnd="@+id/textView19" />

        <EditText
            android:id="@+id/editText15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textEmailAddress"
            android:layout_below="@+id/textView19"
            android:layout_alignRight="@+id/textView19"
            android:layout_alignEnd="@+id/textView19" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:background="@color/colorAccent"
            android:text="تسجيل الدخول"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            tools:layout_width="120dp"
            android:layout_below="@+id/editText15"
            android:layout_alignLeft="@+id/editText15"
            android:layout_alignStart="@+id/editText15" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
